<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="avminWidth" format="dimension" />
        <attr name="avmaxWidth" format="dimension"/>
        <attr name="avminHeight" format="dimension" />
        <attr name="avmaxHeight" format="dimension"/>
        <attr name="avindicatorName" format="string"/>
        <attr name="avindicatorColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference"/>
        <attr name="errorView" format="reference"/>
        <attr name="emptyView" format="reference"/>
        <attr name="noNetworkView" format="reference"/>
        <attr name="contentView" format="reference"/>
    </declare-styleable>
</resources>
